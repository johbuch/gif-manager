{% extends 'base.html.twig' %}

{% block title %}Liste des tags{% endblock %}

{% block body %}
    <div class="container">
        <h1>Liste des tags</h1>
        <ul>
            {% for tag in tags %}
                <li><a href="{{ path('tag_read', {id: tag.id}) }}">{{ tag.name }}</a></li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}
